{
  "name": "runny",
  "version": "1.4.0",
  "description": "run one command in a couple directories",
  "main": "lib/runny.js",
  "bin": {
    "runny": "bin/runny.js"
  },
  "dependencies": {
    "async": "^2.0.0-rc.1",
    "currify": "^1.0.0",
    "minimist": "^1.2.0",
